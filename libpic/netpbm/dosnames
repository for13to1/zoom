#!/bin/sh
# Change pbmplus file names to msdos

TOP=`pwd`
if [ "$1" = "-r" ]; then 
	swap () {
	    echo mv "$1" "$2"
	    mv "$1" "$2"
	}
else
	swap () {
	    echo mv "$2" "$1"
	    mv "$2" "$1"
	}
fi

cd $TOP
swap changes CHANGES
swap formats FORMATS
swap imakefil Imakefile
swap imakefil.tif Imakefile.tiff
swap msdos MSDOS
swap makefile Makefile
swap netpbm Netpbm
swap netpbm.cha Netpbm.Changes
swap netpbm.peo Netpbm.people
swap netpbm.pro Netpbm.programming
swap other.sys OTHER.SYSTEMS
swap pbmplus.tmp Pbmplus.tmpl
swap readme README
swap readme.ami README.AMIGA
swap readme.con README.CONFOCAL
swap readme.vms README.VMS
swap todo TODO
swap doslistm doslistmake
swap stamp-da stamp-date
swap stamp-da.ami stamp-date.amiga
cd $TOP/libtiff
swap makefile Makefile
swap makefile.aix Makefile.aix
swap makefile.apo Makefile.apollo
swap makefile.gcc Makefile.gcc
swap makefile.hpu Makefile.hpux
swap makefile.mms Makefile.mms
swap makefile.mpw Makefile.mpw
swap makefile.nex Makefile.next
swap makefile.sco Makefile.sco
swap makefile.sgi Makefile.sgi
swap makefile.sun Makefile.sun
swap readme README
swap mkg3stat.c mkg3states.c
swap tif_appl.c tif_apple.c
swap tif_ccit.c tif_ccittrle.c
swap tif_clos.c tif_close.c
swap tif_comp.c tif_compress.c
swap tif_diri.c tif_dirinfo.c
swap tif_dirr.c tif_dirread.c
swap tif_dirw.c tif_dirwrite.c
swap tif_dump.c tif_dumpmode.c
swap tif_erro.c tif_error.c
swap tif_flus.c tif_flush.c
swap tif_geti.c tif_getimage.c
swap tif_mach.c tif_machdep.c
swap tif_msdo.c tif_msdos.c
swap tif_pack.c tif_packbits.c
swap tif_prin.c tif_print.c
swap tif_stri.c tif_strip.c
swap tif_thun.c tif_thunder.c
swap tif_vers.c tif_version.c
swap tif_warn.c tif_warning.c
swap tif_writ.c tif_write.c
cd $TOP/pbm
swap imakefil Imakefile
swap makefile Makefile
swap makefile.ami Makefile.amiga
swap makefile.mms Makefile.mms
swap bitrever.h bitreverse.h
swap brushtop.1 brushtopbm.1
swap brushtop.c brushtopbm.c
swap cmuwmtop.1 cmuwmtopbm.1
swap cmuwmtop.c cmuwmtopbm.c
swap icontopb.1 icontopbm.1
swap icontopb.c icontopbm.c
swap libpbm5x.old libpbm5.c.old
swap macptopb.1 macptopbm.1
swap macptopb.c macptopbm.c
swap pbmfontx.old pbmfont.h.old
swap pbmpscal.1 pbmpscale.1
swap pbmpscal.c pbmpscale.c
swap pbmreduc.1 pbmreduce.1
swap pbmreduc.c pbmreduce.c
swap pbmto442.1 pbmto4425.1
swap pbmto442.c pbmto4425.c
swap pbmtoasc.1 pbmtoascii.1
swap pbmtoasc.c pbmtoascii.c
swap pbmtobbn.1 pbmtobbnbg.1
swap pbmtobbn.c pbmtobbnbg.c
swap pbmtocmu.1 pbmtocmuwm.1
swap pbmtocmu.c pbmtocmuwm.c
swap pbmtoeps.1 pbmtoepsi.1
swap pbmtoeps.c pbmtoepsi.c
swap pbmepson.1 pbmtoepson.1
swap pbmepson.c pbmtoepson.c
swap pbmtoico.1 pbmtoicon.1
swap pbmtoico.c pbmtoicon.c
swap pbmtoln0.1 pbmtoln03.1
swap pbmtoln0.c pbmtoln03.c
swap pbmtomac.1 pbmtomacp.1
swap pbmtomac.c pbmtomacp.c
swap pbmtoplo.1 pbmtoplot.1
swap pbmtoplo.c pbmtoplot.c
swap pbmtox10.1 pbmtox10bm.1
swap pbmtox10.c pbmtox10bm.c
swap pbmtozin.1 pbmtozinc.1
swap pbmtozin.c pbmtozinc.c
cd $TOP/pgm
swap imakefil Imakefile
swap makefile Makefile
swap makefile.ami Makefile.amiga
swap makefile.mms Makefile.mms
swap asciitop.1 asciitopgm.1
swap asciitop.c asciitopgm.c
swap bioradto.1 bioradtopgm.1
swap bioradto.c bioradtopgm.c
swap fitstopg.1 fitstopgm.1
swap fitstopg.c fitstopgm.c
swap hipstopg.1 hipstopgm.1
swap hipstopg.c hipstopgm.c
swap lispmtop.1 lispmtopgm.1
swap lispmtop.c lispmtopgm.c
swap pgmbentl.1 pgmbentley.1
swap pgmbentl.c pgmbentley.c
swap pgmcrate.1 pgmcrater.1
swap pgmcrate.c pgmcrater.c
swap pgmenhan.1 pgmenhance.1
swap pgmenhan.c pgmenhance.c
swap pgmtextu.1 pgmtexture.1
swap pgmtextu.c pgmtexture.c
swap pgmtofit.1 pgmtofits.1
swap pgmtofit.c pgmtofits.c
swap pgmtolis.1 pgmtolispm.1
swap pgmtolis.c pgmtolispm.c
swap psidtopg.1 psidtopgm.1
swap psidtopg.c psidtopgm.c
swap spottopg.1 spottopgm.1
swap spottopg.c spottopgm.c
cd $TOP/pnm
swap imakefil Imakefile
swap makefile Makefile
swap makefile.ami Makefile.amiga
swap makefile.mms Makefile.mms
swap pnmconvo.1 pnmconvol.1
swap pnmconvo.c pnmconvol.c
swap pnmenlar.1 pnmenlarge.1
swap pnmenlar.c pnmenlarge.c
swap pnmhistm.1 pnmhistmap.1
swap pnmhistm.c pnmhistmap.c
swap pnminver.1 pnminvert.1
swap pnminver.c pnminvert.c
swap pnmmargi pnmmargin
swap pnmmargi.1 pnmmargin.1
swap pnmnlfil.1 pnmnlfilt.1
swap pnmnlfil.c pnmnlfilt.c
swap pnmrotat.1 pnmrotate.1
swap pnmrotat.c pnmrotate.c
swap pnmsmoot pnmsmooth
swap pnmsmoot.1 pnmsmooth.1
swap pnmtoddi.1 pnmtoddif.1
swap pnmtoddi.c pnmtoddif.c
swap pnmtoras.1 pnmtorast.1
swap pnmtoras.c pnmtorast.c
swap pnmtotif.1 pnmtotiff.1
swap pnmtotif.c pnmtotiff.c
swap rasttopn.1 rasttopnm.1
swap rasttopn.c rasttopnm.c
swap tifftopn.1 tifftopnm.1
swap tifftopn.c tifftopnm.c
swap zeisstop.1 zeisstopnm.1
swap zeisstop.c zeisstopnm.c
cd $TOP/ppm
swap imakefil Imakefile
swap makefile Makefile
swap makefile.ami Makefile.amiga
swap makefile.mms Makefile.mms
swap gouldtop.1 gouldtoppm.1
swap gouldtop.c gouldtoppm.c
swap hpcdtopp.1 hpcdtoppm.1
swap hpcdtopp.c hpcdtoppm.c
