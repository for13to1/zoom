swap ppmrelie.1 ppmrelief.1
swap ppmrelie.c ppmrelief.c
swap ppmsprea.1 ppmspread.1
swap ppmsprea.c ppmspread.c
swap ppmtoaca.1 ppmtoacad.1
swap ppmtoaca.c ppmtoacad.c
swap ppmtoilb.1 ppmtoilbm.1
swap ppmtoilb.c ppmtoilbm.c
swap ppmtomit.1 ppmtomitsu.1
swap ppmtomit.c ppmtomitsu.c
swap ppmtopic.1 ppmtopict.1
swap ppmtopic.c ppmtopict.c
swap ppmtopjx.1 ppmtopjxl.1
swap ppmtopjx.c ppmtopjxl.c
swap ppmtopuz.1 ppmtopuzz.1
swap ppmtopuz.c ppmtopuzz.c
swap ppmtorgb.1 ppmtorgb3.1
swap ppmtorgb.c ppmtorgb3.c
swap ppmtosix.1 ppmtosixel.1
swap ppmtosix.c ppmtosixel.c
swap ppmtoyuv.1 ppmtoyuvsplit.1
swap ppmtoyuv.c ppmtoyuvsplit.c
swap rgb3topp.1 rgb3toppm.1
swap rgb3topp.c rgb3toppm.c
swap yuvsplit.1 yuvsplittoppm.1
swap yuvsplit.c yuvsplittoppm.c
cd $TOP/vms
swap add_list.com Add_List.com
swap make_pbm.com Make_PBMplus.com
swap make_shr.com Make_PBMplusShr.com
swap makefile.mms Makefile.mms
swap netpbm.tex NetPBM.tex
swap pbmplus.hlp PBMplus.hlp
swap rgb.txt RGB.txt
swap setup.com SetUp.com
swap stamp-da.com stamp-date.com
